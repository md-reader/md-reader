    "@mdit/plugin-alert": "^0.22.1",
    "@smui/button": "^4.2.0",
    "@smui/chips": "^4.2.0",
    "@smui/form-field": "^4.2.0",
    "@smui/radio": "^4.2.0",
    "@smui/select": "^4.2.0",
    "@smui/switch": "^4.2.0",
    "@traptitech/markdown-it-katex": "^3.6.0",
    "highlight.js": "^11.6.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "markdown-it": "^13.0.1",
